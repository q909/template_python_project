  langid = {english},
  keywords = {accuracy,aggregations size,distribution system},
  file = {C\:\\Users\\ValgaevO\\Zotero\\storage\\W7SQSWVY\\ztrk et al. - 2013 - Effects of scale on load prediction algorithms.pdf}
